{
  "name": "munakata-kids-university",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "NODE_OPTIONS='--max_old_space_size=4096' astro dev",
    "start": "NODE_OPTIONS='--max_old_space_size=4096' astro dev",
    "build": "NODE_OPTIONS='--max_old_space_size=4096' astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.astro --fix",
    "format": "prettier --write .",
